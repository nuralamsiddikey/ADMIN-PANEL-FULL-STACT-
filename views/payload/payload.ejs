    <a href="/adminPanel" class="text-white "><i class="fa fa-server mx-2" aria-hidden="true"></i> Home</a>

    <a id="server_menu_ml" href="/server" class="text-white "><i class="fa fa-server mx-2" aria-hidden="true"></i> Server</a>
    <a id="payload_menu_ml" href="/payload" class="text-white"><i class="fa fa-street-view mx-2" aria-hidden="true"></i> Payload</a>
    <a id="config_menu_ml" href="/config" class="text-white"><i class="fa fa-cogs mx-2" aria-hidden="true"></i> Config</a>

    <form action="/logout" class="mx-4 mt-4">
      <button type="submit" style="background-color: #3498db; border: 0; color: white;"><i class="fa fa-sign-out mx-1"
          aria-hidden="true"></i>Logout</button>
    </form>

  </div>


  <div class="main-content">

    <div class="header shadow-lg rounded">

      <div class="header-left">
        <h4>Admin Panel</h4>
      </div>


      <div class="header-right">
        <form action="/logout" class="logout">
          <button type="submit" style="background-color: #3498db; border: 0; color: white;"><i
              class="fa fa-sign-out mx-1" aria-hidden="true"></i>Logout</button>
        </form>
        <!-- <div class="logout">
          <a href="#"></i>Logout</a>
        </div> -->

        <div class="menu" onclick="showSidebar()">
          <i class="fa fa-bars" aria-hidden="true"></i>
        </div>

      </div>


    </div>


    
 
    <div class="card mt-4 shadow-lg">
      <div class="card-header ">
        <h5 class="float-start">Payload</h5>
        <button type="button" class="btn btn-primary float-end" data-bs-toggle="modal" data-bs-target="#exampleModal">
        Add Payload
        </button>
       
      </div>
      <div class="card-body"> 

        
        <table class="table table-bordered table-hover table-responsive">
          <thead >
            <tr style="font-size: 14px;">
              <th scope="col">Sl</th>
              <th scope="col">Name</th>
              <th scope="col">Info.</th>
              <th scope="col">Custom Proxy</th>
              <th scope="col">Proxy Port</th>
              <th scope="col">Type</th>
              <th scope="col">Tweak</th>
              <th scope="col">Flag</th>
              <th scope="col">Action</th>
            </tr>
          </thead>
          <tbody id="payload_table">
           
           
          </tbody>
        </table>
      </div>

 <div class="card-footer" >


  <nav aria-label="Page navigation example">
    <ul class="pagination" id="payload_table_pagination">
      
   
     
    </ul>
  </nav>



      </div>
    </div>
  
  </div>

  <div >


  </div>

  </div>


  
 
 <!-- CREATE PAYLOAD MODAL -->

<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content" id="payload_modal_dismiss">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        

        <form >


          <label for="payloadName">Payload Name</label>
          <input
          name="name"
           type="text" 
           id="payloadName" 
           placeholder="Payload name"
           class="form-control mb-1"
           >
           
           <label for="info">Info.</label>
           <textarea
           name="info"
            type="text" 
            id="info" 
            placeholder="Text"
            class="form-control mb-1"
            ></textarea>
           
            <label for="customProxy">Custom Proxy</label>
            <input
            
             type="number" 
             id="customProxy" 
             placeholder="Custom proxy"
             class="form-control mb-1"
             >
             
             <label for="proxyPort">Proxy Port</label>
             <input
            
              type="number" 
              id="proxyPort" 
              placeholder="Proxy port"
              class="form-control mb-1"
              >
              
            
          <label for="type">Type</label>
          <select class="form-select form-select-md mb-3" 
          id="type"
          aria-label=".form-select-lg example">
           
           <option value="http">HTTP</option>
           <option value="ssl">SSL</option>
         
         </select>
         
         <label for="tweak">Tweak</label>
         <input
        
      
          id="tweak" 
          placeholder="Teaks"
          class="form-control form-control-lg mb-1"
          >

       
          

            <label for="type">Flag</label>
            <select class="form-select form-select-md mb-3" 
            id="flag"
            aria-label=".form-select-lg example">
             
             <option value="All">All</option>
            
           </select>



        </form>


      </div>
      <div class="modal-footer">
       
        <button onclick="create_payload()" type="button" class="btn btn-primary">Create</button>
      </div>
    </div>
  </div>
</div>


 <!-- UPDATE PAYLOAD MODAL -->

<!-- Modal -->
<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content" id="update_payload">
      <div class="modal-header">
        <h5 class="modal-title" id="staticBackdropLabel">Update Payload</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body" id="payload_update_modal">
        
      </div>
      
    </div>
  </div>
</div>






